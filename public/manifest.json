{
  "name": "Pitched",
  "version": "0.1.0",
  "description": "Toggles the preserve-pitch algorithm",
  "manifest_version": 2,
  "permissions": ["<all_urls>", "tabs"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["index.js"]
    }
  ],
  "web_accessible_resources": ["context.js"],
  "browser_action": {
    "default_icon": "icon-on.svg"
  }
}
